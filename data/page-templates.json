{
  "_metadata": {
    "description": "Page layout templates for consistent page generation. Each template provides a complete JSX structure that Claude can adapt to specific content while maintaining consistent layout patterns.",
    "version": "1.0.0",
    "usage": "Use get_page_template tool to retrieve a template, then adapt slots to your specific content needs."
  },
  "DashboardPage": {
    "description": "Standard dashboard page with metrics grid, charts, and content sections",
    "when": "Use for any analytics, overview, or metrics-focused dashboard page",
    "jsx": "<>\n  <PageHeader\n    title=\"{pageTitle}\"\n    subtitle=\"{pageSubtitle}\"\n    icon={{pageIcon}}\n    actions={\n      <div className=\"flex items-center gap-2\">\n        <Select defaultValue=\"7d\">\n          <SelectTrigger className=\"w-[180px]\">\n            <SelectValue placeholder=\"Período\" />\n          </SelectTrigger>\n          <SelectContent>\n            <SelectItem value=\"7d\">Últimos 7 dias</SelectItem>\n            <SelectItem value=\"30d\">Últimos 30 dias</SelectItem>\n            <SelectItem value=\"90d\">Últimos 90 dias</SelectItem>\n          </SelectContent>\n        </Select>\n        <Button variant=\"outline\">\n          <Download className=\"h-4 w-4\" />\n          Exportar\n        </Button>\n      </div>\n    }\n  />\n\n  <PageContainer>\n    <div className=\"space-y-8\">\n      {/* Metrics Grid - 4 columns */}\n      <div className=\"grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-4\">\n        <MetricCard icon={DollarSign} label=\"{metric1Label}\" value=\"{metric1Value}\" trend={{ value: 15, direction: 'up' }} />\n        <MetricCard icon={Users} label=\"{metric2Label}\" value=\"{metric2Value}\" trend={{ value: 8, direction: 'up' }} />\n        <MetricCard icon={BarChart3} label=\"{metric3Label}\" value=\"{metric3Value}\" trend={{ value: 3.2, direction: 'up' }} />\n        <MetricCard icon={Package} label=\"{metric4Label}\" value=\"{metric4Value}\" />\n      </div>\n\n      {/* Main Chart */}\n      <Card className=\"p-6\">\n        <div className=\"flex items-center justify-between mb-6\">\n          <h2 className=\"text-lg font-semibold\">{chartTitle}</h2>\n          <Select defaultValue=\"7d\">\n            <SelectTrigger className=\"w-[140px]\">\n              <SelectValue />\n            </SelectTrigger>\n            <SelectContent>\n              <SelectItem value=\"7d\">7 dias</SelectItem>\n              <SelectItem value=\"30d\">30 dias</SelectItem>\n            </SelectContent>\n          </Select>\n        </div>\n        <div className=\"h-[350px]\">\n          <ResponsiveContainer width=\"100%\" height=\"100%\">\n            <AreaChart data={chartData}>\n              <CartesianGrid strokeDasharray=\"3 3\" className=\"stroke-muted\" />\n              <XAxis dataKey=\"date\" className=\"text-xs\" />\n              <YAxis className=\"text-xs\" />\n              <Tooltip />\n              <Area type=\"monotone\" dataKey=\"value\" stroke=\"hsl(var(--primary))\" fill=\"hsl(var(--primary)/0.1)\" />\n            </AreaChart>\n          </ResponsiveContainer>\n        </div>\n      </Card>\n\n      {/* Two Column Section */}\n      <div className=\"grid grid-cols-1 lg:grid-cols-2 gap-6\">\n        <Card className=\"p-6\">\n          <h2 className=\"text-lg font-semibold mb-4\">{leftSectionTitle}</h2>\n          {/* Left content: table, list, or chart */}\n        </Card>\n        <Card className=\"p-6\">\n          <h2 className=\"text-lg font-semibold mb-4\">{rightSectionTitle}</h2>\n          {/* Right content: table, list, or chart */}\n        </Card>\n      </div>\n    </div>\n  </PageContainer>\n</>",
    "slots": {
      "pageTitle": "string - Page title (e.g., 'Analytics')",
      "pageSubtitle": "string - Page subtitle (e.g., 'Acompanhe suas métricas')",
      "pageIcon": "LucideIcon - Page icon (e.g., BarChart3)",
      "metric1-4": "MetricCard props - icon, label, value, trend",
      "chartTitle": "string - Main chart title",
      "chartData": "array - Data for the chart",
      "leftSectionTitle": "string - Left column title",
      "rightSectionTitle": "string - Right column title"
    },
    "patterns": {
      "metricsGrid": "grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-4",
      "twoColumns": "grid grid-cols-1 lg:grid-cols-2 gap-6",
      "threeColumns": "grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6",
      "sectionSpacing": "space-y-8",
      "cardChart": "Card p-6 > [header flex justify-between mb-6, chart h-[350px]]"
    }
  },
  "ListPage": {
    "description": "Page with data table, search, filters, and CRUD actions",
    "when": "Use for any page listing items (products, users, orders, leads)",
    "jsx": "<>\n  <PageHeader\n    title=\"{pageTitle}\"\n    subtitle=\"{pageSubtitle}\"\n    icon={{pageIcon}}\n    actions={\n      <Button>\n        <Plus className=\"h-4 w-4\" />\n        {addButtonLabel}\n      </Button>\n    }\n  />\n\n  <PageContainer>\n    <Card className=\"p-6\">\n      {/* Toolbar */}\n      <div className=\"flex flex-col sm:flex-row items-start sm:items-center justify-between gap-4 mb-6\">\n        <div className=\"relative w-full sm:max-w-sm\">\n          <Search className=\"absolute left-3 top-1/2 -translate-y-1/2 h-4 w-4 text-muted-foreground\" />\n          <Input className=\"pl-10\" placeholder=\"Buscar {searchPlaceholder}...\" value={search} onChange={(e) => setSearch(e.target.value)} />\n        </div>\n        <div className=\"flex items-center gap-2\">\n          <Select defaultValue=\"all\">\n            <SelectTrigger className=\"w-[140px]\">\n              <SelectValue placeholder=\"Status\" />\n            </SelectTrigger>\n            <SelectContent>\n              <SelectItem value=\"all\">Todos</SelectItem>\n              <SelectItem value=\"active\">Ativos</SelectItem>\n              <SelectItem value=\"inactive\">Inativos</SelectItem>\n            </SelectContent>\n          </Select>\n        </div>\n      </div>\n\n      {/* Table */}\n      <div className=\"rounded-md border\">\n        <Table>\n          <TableHeader>\n            <TableRow>\n              <TableHead className=\"w-[50px]\"><Checkbox /></TableHead>\n              <TableHead>{col1Header}</TableHead>\n              <TableHead>{col2Header}</TableHead>\n              <TableHead>{col3Header}</TableHead>\n              <TableHead className=\"text-right\">Ações</TableHead>\n            </TableRow>\n          </TableHeader>\n          <TableBody>\n            {items.map((item) => (\n              <TableRow key={item.id}>\n                <TableCell><Checkbox /></TableCell>\n                <TableCell className=\"font-medium\">{item.col1}</TableCell>\n                <TableCell>{item.col2}</TableCell>\n                <TableCell><Badge variant={item.statusVariant}>{item.status}</Badge></TableCell>\n                <TableCell className=\"text-right\">\n                  <DropdownMenu>\n                    <DropdownMenuTrigger asChild>\n                      <Button variant=\"ghost\" size=\"icon\"><MoreHorizontal className=\"h-4 w-4\" /></Button>\n                    </DropdownMenuTrigger>\n                    <DropdownMenuContent align=\"end\">\n                      <DropdownMenuItem><Eye className=\"h-4 w-4\" /> Ver detalhes</DropdownMenuItem>\n                      <DropdownMenuItem><Pencil className=\"h-4 w-4\" /> Editar</DropdownMenuItem>\n                      <DropdownMenuSeparator />\n                      <DropdownMenuItem className=\"text-destructive\"><Trash2 className=\"h-4 w-4\" /> Excluir</DropdownMenuItem>\n                    </DropdownMenuContent>\n                  </DropdownMenu>\n                </TableCell>\n              </TableRow>\n            ))}\n          </TableBody>\n        </Table>\n      </div>\n\n      {/* Pagination */}\n      <div className=\"flex items-center justify-between mt-4\">\n        <p className=\"text-sm text-muted-foreground\">Mostrando {start}-{end} de {total} resultados</p>\n        <div className=\"flex items-center gap-2\">\n          <Button variant=\"outline\" size=\"icon\" disabled={page === 1}><ChevronLeft className=\"h-4 w-4\" /></Button>\n          <Button variant=\"outline\" size=\"icon\" disabled={page === totalPages}><ChevronRight className=\"h-4 w-4\" /></Button>\n        </div>\n      </div>\n    </Card>\n  </PageContainer>\n\n  {/* Delete Confirmation Dialog */}\n  <Dialog open={deleteOpen} onOpenChange={setDeleteOpen}>\n    <DialogContent>\n      <DialogHeader>\n        <DialogTitle>Confirmar exclusão</DialogTitle>\n        <DialogDescription>Tem certeza que deseja excluir este item? Esta ação não pode ser desfeita.</DialogDescription>\n      </DialogHeader>\n      <DialogFooter>\n        <Button variant=\"outline\" onClick={() => setDeleteOpen(false)}>Cancelar</Button>\n        <Button variant=\"destructive\" onClick={handleDelete}>Excluir</Button>\n      </DialogFooter>\n    </DialogContent>\n  </Dialog>\n</>",
    "slots": {
      "pageTitle": "string",
      "pageSubtitle": "string",
      "pageIcon": "LucideIcon",
      "addButtonLabel": "string (e.g., 'Novo Produto')",
      "searchPlaceholder": "string (e.g., 'produtos')",
      "columns": "TableHead labels",
      "items": "Array of data to display"
    }
  },
  "FormPage": {
    "description": "Page with a form for creating or editing an item",
    "when": "Use for settings, create/edit pages, profile pages",
    "jsx": "<>\n  <PageHeader\n    title=\"{pageTitle}\"\n    subtitle=\"{pageSubtitle}\"\n    icon={{pageIcon}}\n    backButton={{ href: '{backHref}', label: '{backLabel}' }}\n    actions={\n      <div className=\"flex items-center gap-2\">\n        <Button variant=\"outline\" onClick={() => router.back()}>Cancelar</Button>\n        <Button onClick={handleSubmit} disabled={loading}>\n          {loading ? (<><Loader2 className=\"h-4 w-4 animate-spin\" /> Salvando...</>) : 'Salvar'}\n        </Button>\n      </div>\n    }\n  />\n\n  <PageContainer variant=\"narrow\">\n    <div className=\"space-y-6\">\n      {/* Section 1 */}\n      <Card>\n        <CardHeader>\n          <CardTitle className=\"text-lg\">{section1Title}</CardTitle>\n          <CardDescription>{section1Description}</CardDescription>\n        </CardHeader>\n        <CardContent className=\"space-y-4\">\n          <div className=\"grid grid-cols-1 md:grid-cols-2 gap-4\">\n            <div className=\"space-y-2\">\n              <Label htmlFor=\"field1\">{field1Label}</Label>\n              <Input id=\"field1\" placeholder=\"{field1Placeholder}\" />\n            </div>\n            <div className=\"space-y-2\">\n              <Label htmlFor=\"field2\">{field2Label}</Label>\n              <Input id=\"field2\" placeholder=\"{field2Placeholder}\" />\n            </div>\n          </div>\n          <div className=\"space-y-2\">\n            <Label htmlFor=\"field3\">{field3Label}</Label>\n            <Textarea id=\"field3\" placeholder=\"{field3Placeholder}\" />\n          </div>\n        </CardContent>\n      </Card>\n\n      {/* Section 2 */}\n      <Card>\n        <CardHeader>\n          <CardTitle className=\"text-lg\">{section2Title}</CardTitle>\n          <CardDescription>{section2Description}</CardDescription>\n        </CardHeader>\n        <CardContent className=\"space-y-4\">\n          <div className=\"flex items-center justify-between\">\n            <div className=\"space-y-0.5\">\n              <Label>{toggleLabel}</Label>\n              <p className=\"text-sm text-muted-foreground\">{toggleDescription}</p>\n            </div>\n            <Switch />\n          </div>\n          <Separator />\n          <div className=\"space-y-2\">\n            <Label>{selectLabel}</Label>\n            <Select>\n              <SelectTrigger><SelectValue placeholder=\"Selecione...\" /></SelectTrigger>\n              <SelectContent>\n                <SelectItem value=\"opt1\">{option1}</SelectItem>\n                <SelectItem value=\"opt2\">{option2}</SelectItem>\n              </SelectContent>\n            </Select>\n          </div>\n        </CardContent>\n      </Card>\n    </div>\n  </PageContainer>\n</>",
    "slots": {
      "pageTitle": "string",
      "backHref": "string - URL to go back to",
      "backLabel": "string - Back button text",
      "sections": "Array of form sections with fields"
    }
  },
  "SettingsPage": {
    "description": "Settings page with tabs for different configuration sections",
    "when": "Use for settings, preferences, configuration pages",
    "jsx": "<>\n  <PageHeader\n    title=\"Configurações\"\n    subtitle=\"Gerencie as configurações do sistema\"\n    icon={Settings}\n  />\n\n  <PageContainer>\n    <Tabs defaultValue=\"general\">\n      <TabsList>\n        <TabsTrigger value=\"general\" className=\"gap-2\">\n          <Settings className=\"h-4 w-4\" /> Geral\n        </TabsTrigger>\n        <TabsTrigger value=\"integrations\" className=\"gap-2\">\n          <Webhook className=\"h-4 w-4\" /> Integrações\n        </TabsTrigger>\n        <TabsTrigger value=\"billing\" className=\"gap-2\">\n          <CreditCard className=\"h-4 w-4\" /> Pagamento\n        </TabsTrigger>\n      </TabsList>\n\n      <TabsContent value=\"general\" className=\"mt-6\">\n        <div className=\"space-y-6\">\n          <Card>\n            <CardHeader>\n              <CardTitle className=\"text-lg\">Informações Gerais</CardTitle>\n              <CardDescription>Configurações básicas do sistema</CardDescription>\n            </CardHeader>\n            <CardContent className=\"space-y-4\">\n              {/* Form fields */}\n            </CardContent>\n          </Card>\n        </div>\n      </TabsContent>\n\n      <TabsContent value=\"integrations\" className=\"mt-6\">\n        {/* Integration settings */}\n      </TabsContent>\n\n      <TabsContent value=\"billing\" className=\"mt-6\">\n        {/* Billing settings */}\n      </TabsContent>\n    </Tabs>\n  </PageContainer>\n</>",
    "slots": {
      "tabs": "Array of tab definitions with icon, label, and content"
    }
  },
  "LoginPage": {
    "description": "Authentication login page with centered card",
    "when": "Use for login, register, forgot password pages",
    "jsx": "<div className=\"min-h-screen flex items-center justify-center bg-slate-50 dark:bg-slate-950 p-4\">\n  <div className=\"w-full max-w-md\">\n    {/* Header */}\n    <div className=\"text-center mb-8\">\n      <div className=\"inline-flex items-center justify-center w-16 h-16 rounded-2xl bg-gradient-to-br from-slate-100 to-slate-200 dark:from-slate-800 dark:to-slate-700 mb-6 shadow-sm\">\n        <LogIn className=\"w-8 h-8 text-slate-700 dark:text-slate-200\" />\n      </div>\n      <h1 className=\"text-3xl font-bold text-slate-900 dark:text-slate-100\">Bem-vindo de volta</h1>\n      <p className=\"text-slate-600 dark:text-slate-400 mt-2\">Entre com suas credenciais para acessar o painel</p>\n    </div>\n\n    {/* Card */}\n    <div className=\"bg-white dark:bg-slate-800 rounded-xl shadow-sm border border-slate-200 dark:border-slate-700 p-8\">\n      {error && (\n        <Alert variant=\"destructive\" className=\"mb-6\">\n          <AlertCircle className=\"h-4 w-4\" />\n          <AlertDescription>{error}</AlertDescription>\n        </Alert>\n      )}\n\n      <form onSubmit={handleSubmit} className=\"space-y-6\">\n        <div className=\"space-y-2\">\n          <Label htmlFor=\"email\">Email</Label>\n          <div className=\"relative\">\n            <Mail className=\"absolute left-3 top-1/2 -translate-y-1/2 h-4 w-4 text-muted-foreground\" />\n            <Input id=\"email\" type=\"email\" className=\"pl-10\" placeholder=\"seu@email.com\" value={email} onChange={(e) => setEmail(e.target.value)} required />\n          </div>\n        </div>\n\n        <div className=\"space-y-2\">\n          <Label htmlFor=\"password\">Senha</Label>\n          <div className=\"relative\">\n            <Lock className=\"absolute left-3 top-1/2 -translate-y-1/2 h-4 w-4 text-muted-foreground\" />\n            <Input id=\"password\" type=\"password\" className=\"pl-10\" placeholder=\"••••••••\" value={password} onChange={(e) => setPassword(e.target.value)} required />\n          </div>\n        </div>\n\n        <Button type=\"submit\" className=\"w-full\" disabled={loading}>\n          {loading ? (<><Loader2 className=\"h-4 w-4 animate-spin\" /> Entrando...</>) : 'Entrar'}\n        </Button>\n      </form>\n    </div>\n\n    <p className=\"text-center text-sm text-slate-600 dark:text-slate-400 mt-6\">\n      Não tem uma conta?{' '}<Link href=\"/register\" className=\"text-primary hover:underline font-medium\">Cadastre-se</Link>\n    </p>\n  </div>\n</div>",
    "slots": {
      "fields": "Form fields (email, password, etc.)",
      "onSubmit": "Form submission handler",
      "links": "Footer links (register, forgot password)"
    }
  },
  "DetailPage": {
    "description": "Detail/view page for a single item with info cards and actions",
    "when": "Use for product detail, user profile, order detail pages",
    "jsx": "<>\n  <PageHeader\n    title=\"{itemName}\"\n    subtitle=\"{itemDescription}\"\n    icon={{pageIcon}}\n    backButton={{ href: '{listHref}', label: 'Voltar para {listName}' }}\n    actions={\n      <div className=\"flex items-center gap-2\">\n        <Button variant=\"outline\"><Pencil className=\"h-4 w-4\" /> Editar</Button>\n        <Button variant=\"destructive\"><Trash2 className=\"h-4 w-4\" /> Excluir</Button>\n      </div>\n    }\n  />\n\n  <PageContainer>\n    <div className=\"space-y-6\">\n      {/* Info Cards Grid */}\n      <div className=\"grid grid-cols-1 md:grid-cols-3 gap-4\">\n        <MetricCard icon={DollarSign} label=\"{stat1Label}\" value=\"{stat1Value}\" />\n        <MetricCard icon={Users} label=\"{stat2Label}\" value=\"{stat2Value}\" />\n        <MetricCard icon={BarChart3} label=\"{stat3Label}\" value=\"{stat3Value}\" />\n      </div>\n\n      {/* Main Content */}\n      <div className=\"grid grid-cols-1 lg:grid-cols-3 gap-6\">\n        {/* Main Column (2/3) */}\n        <div className=\"lg:col-span-2 space-y-6\">\n          <Card>\n            <CardHeader>\n              <CardTitle className=\"text-lg\">Informações</CardTitle>\n            </CardHeader>\n            <CardContent>\n              <dl className=\"space-y-4\">\n                <div className=\"flex justify-between\">\n                  <dt className=\"text-sm text-muted-foreground\">{field1Label}</dt>\n                  <dd className=\"text-sm font-medium\">{field1Value}</dd>\n                </div>\n                <Separator />\n                <div className=\"flex justify-between\">\n                  <dt className=\"text-sm text-muted-foreground\">{field2Label}</dt>\n                  <dd className=\"text-sm font-medium\">{field2Value}</dd>\n                </div>\n              </dl>\n            </CardContent>\n          </Card>\n        </div>\n\n        {/* Sidebar (1/3) */}\n        <div className=\"space-y-6\">\n          <Card className=\"p-6\">\n            <h3 className=\"font-semibold mb-4\">Status</h3>\n            <Badge variant=\"success\">Ativo</Badge>\n          </Card>\n          <Card className=\"p-6\">\n            <h3 className=\"font-semibold mb-4\">Ações Rápidas</h3>\n            <div className=\"space-y-2\">\n              <Button variant=\"outline\" className=\"w-full justify-start\"><Copy className=\"h-4 w-4\" /> Duplicar</Button>\n              <Button variant=\"outline\" className=\"w-full justify-start\"><Share2 className=\"h-4 w-4\" /> Compartilhar</Button>\n            </div>\n          </Card>\n        </div>\n      </div>\n    </div>\n  </PageContainer>\n</>",
    "slots": {
      "itemName": "string - Item title",
      "listHref": "string - Back to list URL",
      "stats": "MetricCard props for overview stats",
      "fields": "Detail field labels and values",
      "sidebarContent": "Sidebar cards content"
    }
  },
  "LandingPage": {
    "description": "Complete landing page layout with all sections",
    "when": "Use for marketing, sales, or product landing pages",
    "jsx": "<div className=\"min-h-screen bg-slate-950 text-white\">\n  {/* Hero Section */}\n  <section className=\"relative min-h-screen flex items-start justify-center overflow-hidden pt-20 md:pt-24\">\n    <div className=\"absolute inset-0 opacity-5\" style={{ backgroundImage: 'radial-gradient(circle at 1px 1px, white 1px, transparent 0)', backgroundSize: '40px 40px' }} />\n    <div className=\"absolute top-1/4 left-1/2 -translate-x-1/2 w-[600px] h-[600px] rounded-full bg-cyan-500/10 blur-[150px]\" />\n    <div className=\"container relative z-10 text-center px-4\">\n      <span className=\"inline-block px-4 py-2 mb-6 text-sm font-medium tracking-wider uppercase border border-cyan-500/30 text-cyan-400 rounded-full\">{badgeText}</span>\n      <h1 className=\"font-bold font-heading text-5xl md:text-7xl lg:text-8xl mb-6\">\n        <span className=\"text-gradient-primary\">{heroKeyword}</span> {heroTitle}\n      </h1>\n      <p className=\"max-w-2xl mx-auto text-xl md:text-2xl text-gray-400 mb-12\">{heroSubtitle}</p>\n      <button className=\"group inline-flex items-center gap-3 px-8 py-4 text-lg font-semibold rounded-lg bg-cyan-500 hover:bg-cyan-600 text-white shadow-lg shadow-cyan-500/20 transition-all duration-300\">\n        {ctaText} <ArrowDown className=\"w-5 h-5 group-hover:translate-y-1 transition-transform\" />\n      </button>\n    </div>\n  </section>\n\n  {/* Stats Section */}\n  <section className=\"py-16 border-t border-neutral-800\">\n    <div className=\"container mx-auto px-4\">\n      <div className=\"grid grid-cols-2 md:grid-cols-4 gap-8 text-center\">\n        <div>\n          <p className=\"text-3xl md:text-5xl font-bold text-gradient-primary\">{stat1Value}</p>\n          <p className=\"mt-2 text-gray-400\">{stat1Label}</p>\n        </div>\n        {/* More stats... */}\n      </div>\n    </div>\n  </section>\n\n  {/* Benefits Section */}\n  <section className=\"py-16 md:py-24\">\n    <div className=\"container mx-auto px-4\">\n      <div className=\"text-center mb-12\">\n        <h2 className=\"text-2xl md:text-4xl font-bold mb-4\">Por que escolher <span className=\"text-gradient-primary\">nossa plataforma</span></h2>\n        <p className=\"max-w-2xl mx-auto text-muted-foreground\">{benefitsSubtitle}</p>\n      </div>\n      <div className=\"grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6\">\n        {benefits.map((b, i) => (\n          <div key={i} className=\"group p-6 rounded-xl border bg-neutral-900/50 border-neutral-800 hover:border-primary/50 transition-all duration-300\">\n            <div className=\"inline-flex items-center justify-center w-12 h-12 rounded-lg bg-primary/10 mb-4\">\n              <b.icon className=\"w-6 h-6 text-primary\" />\n            </div>\n            <h3 className=\"text-lg font-semibold mb-2\">{b.title}</h3>\n            <p className=\"text-sm text-muted-foreground\">{b.description}</p>\n            <div className=\"h-0.5 w-0 group-hover:w-full bg-gradient-to-r from-primary to-secondary transition-all duration-300 mt-4\" />\n          </div>\n        ))}\n      </div>\n    </div>\n  </section>\n\n  {/* Plans Section */}\n  <section id=\"plans\" className=\"py-24\">\n    <div className=\"container mx-auto px-4\">\n      <div className=\"text-center mb-12\">\n        <h2 className=\"text-2xl md:text-4xl font-bold mb-4\">Escolha seu <span className=\"text-gradient-primary\">plano</span></h2>\n      </div>\n      <div className=\"grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 max-w-5xl mx-auto gap-4 sm:gap-6\">\n        {/* PlanCard components */}\n      </div>\n    </div>\n  </section>\n\n  {/* CTA Section */}\n  <section className=\"py-16 md:py-24 bg-primary text-primary-foreground\">\n    <div className=\"container mx-auto px-4 text-center max-w-2xl\">\n      <h2 className=\"text-2xl md:text-4xl font-bold mb-4\">{ctaSectionTitle}</h2>\n      <p className=\"mb-8 opacity-90\">{ctaSectionDescription}</p>\n      <button className=\"inline-flex items-center gap-2 px-8 py-4 rounded-lg font-semibold bg-white text-primary hover:bg-gray-100 transition-all\">\n        {ctaButtonText} <ArrowRight className=\"w-5 h-5\" />\n      </button>\n    </div>\n  </section>\n\n  {/* Footer */}\n  <footer className=\"py-8 border-t border-neutral-800\">\n    <div className=\"container mx-auto px-4 text-center text-sm text-gray-400\">\n      <p>© {year} {companyName}. Todos os direitos reservados.</p>\n    </div>\n  </footer>\n</div>",
    "slots": {
      "badgeText": "string - Hero badge text",
      "heroTitle": "string - Main headline",
      "heroSubtitle": "string - Secondary headline",
      "products": "Array of products for PlanCards",
      "benefits": "Array of benefits for BenefitsSection",
      "testimonials": "Array of testimonials"
    },
    "sections": ["HeroSection", "StatsSection", "BenefitsSection", "PlanCardsGrid", "TestimonialsSection", "CTASection", "Footer"]
  },
  "EmptyState": {
    "description": "Empty state for pages with no data yet",
    "when": "Use when a list or dashboard has no data to display",
    "jsx": "<div className=\"flex flex-col items-center justify-center py-16 text-center\">\n  <div className=\"inline-flex items-center justify-center w-16 h-16 rounded-2xl bg-muted mb-6\">\n    <Icon className=\"w-8 h-8 text-muted-foreground\" />\n  </div>\n  <h3 className=\"text-lg font-semibold mb-2\">{title}</h3>\n  <p className=\"text-sm text-muted-foreground max-w-md mb-6\">{description}</p>\n  <Button>\n    <Plus className=\"h-4 w-4\" />\n    {actionLabel}\n  </Button>\n</div>",
    "slots": {
      "icon": "LucideIcon",
      "title": "string (e.g., 'Nenhum produto encontrado')",
      "description": "string",
      "actionLabel": "string (e.g., 'Criar primeiro produto')"
    }
